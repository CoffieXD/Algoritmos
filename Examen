    nombreEquipo: "Equipo 4",
    integrantes: [Maria,Karen,Angeles,Lupita]
});

equipos.push({
    nombreEquipo: "Equipo 5",
    integrantes: [Carmen,Lupita,Marta,Alice]
});

equipos.push({
    nombreEquipo: "Equipo 6",
    integrantes: [Fernanda,Roxana,Estefani]
});

document.getElementById("buscarForm").onsubmit = function(e) {
    e.preventDefault();

    if(document.getElementById("tblEquipo") != undefined){
        document.getElementById("tblEquipo").remove();
    }
    
    let nombreEquipo = document.getElementById("nombreEquipo").value;
    equipoBuscado = equipos.find(equipo => equipo.nombreEquipo === nombreEquipo)
    if(equipoBuscado != undefined){
        document.getElementById("Resultado").textContent = "::Integrantes del equipo::";
        mostrarInformacionEquipo(equipoBuscado);
    }else{
        document.getElementById("Resultado").textContent = "No se encontro el equipo!";
    }
}

//https://developer.mozilla.org/es/docs/Web/API/Document_Object_Model/Traversing_an_HTML_table_with_JavaScript_and_DOM_Interfaces
function mostrarInformacionEquipo(equipoBuscado) {
    var body = document.getElementsByTagName("body")[0];
  
    var tabla   = document.createElement("table");
    tabla.setAttribute("id","tblEquipo");
    var tblBody = document.createElement("tbody");
    
    console.log(equipoBuscado);

    equipoBuscado.integrantes.forEach(integrante => {
        var tr = document.createElement("tr");
        var nombreTD = document.createElement("td");
        var textoNombre = document.createTextNode("Nombre");
        nombreTD.appendChild(textoNombre);
        tr.appendChild(nombreTD);

        var celdaNombre = document.createElement("td");
        var textoCeldaNombre = document.createTextNode(integrante.nombre);
        celdaNombre.appendChild(textoCeldaNombre);
        tr.appendChild(celdaNombre);

        var edadTD = document.createElement("td");
        var textoEdad = document.createTextNode("Edad");
        edadTD.appendChild(textoEdad);
        tr.appendChild(edadTD);

        var celdaEdad = document.createElement("td");
        var textoCeldaEdad = document.createTextNode(integrante.edad);
        celdaEdad.appendChild(textoCeldaEdad);
        tr.appendChild(celdaEdad);
        tblBody.appendChild(tr);

        var registroTD = document.createElement("td");
        var textoRegistro = document.createTextNode("Registro");
        registroTD.appendChild(textoRegistro);
        tr.appendChild(registroTD);

        var celdaRegistro = document.createElement("td");
        var textoCeldaRegistro = document.createTextNode(integrante.registro);
        celdaRegistro.appendChild(textoCeldaRegistro);
        tr.appendChild(celdaRegistro);
        
        
        var sexoTD = document.createElement("td");
        var textoSexo = document.createTextNode("Sexo");
        sexoTD.appendChild(textoSexo);
        tr.appendChild(sexoTD);

        var celdaSexo = document.createElement("td");
        var textoCeldaSexo = document.createTextNode(integrante.sexo);
        celdaSexo.appendChild(textoCeldaSexo);
        tr.appendChild(celdaSexo);

        
        var telefonoTD = document.createElement("td");
        var textoTelefono = document.createTextNode("Telefono");
        telefonoTD.appendChild(textoTelefono);
        tr.appendChild(telefonoTD);

        var celdaTelefono = document.createElement("td");
        var textoCeldaTelefono = document.createTextNode(integrante.telefono);
        celdaTelefono.appendChild(textoCeldaTelefono);
        tr.appendChild(celdaTelefono);
        
        var habilidadesTD = document.createElement("td");
        var textoHabilidades = document.createTextNode("Habilidades");
        habilidadesTD.appendChild(textoHabilidades);
        tr.appendChild(habilidadesTD);

        var celdaHabilidades = document.createElement("td");
        var textoCeldaHabilidades = document.createTextNode(integrante.habilidades);
        celdaHabilidades.appendChild(textoCeldaHabilidades);
        tr.appendChild(celdaHabilidades);

        
        var lugarhajugadoTD = document.createElement("td");
        var textoLugarhajugado = document.createTextNode("Lugar donde ha jugado");
        lugarhajugadoTD.appendChild(textoLugarhajugado);
        tr.appendChild(lugarhajugadoTD);

        var celdaLugarhajugado = document.createElement("td");
        var textoCeldaLugarhajugado = document.createTextNode(integrante.lugarhajugado);
        celdaLugarhajugado.appendChild(textoCeldaLugarhajugado);
        tr.appendChild(celdaLugarhajugado);
    });
    
    
    tabla.appendChild(tblBody);
    body.appendChild(tabla);
    tabla.setAttribute("border", "2");
  }
